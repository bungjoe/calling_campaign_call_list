CREATE TABLE "AP_CRM"."LOG_ELIG_DAILY_CHECK" 
   (	
      "LOG_DATE" DATE, 
      "TIME_INSERTED" VARCHAR2(8 BYTE), 
      "CAMPAIGN_ID" VARCHAR2(4 BYTE), 
      "ID_CUID" NUMBER(38,0), 
      "MAX_CREDIT_AMOUNT" NUMBER, 
      "MAX_ANNUITY" NUMBER, 
      "VALIDITY_PERIOD" VARCHAR2(4 BYTE), 
      "DPD_HISTORY" NUMBER(38,0), 
      "DPD_12_MS" NUMBER(38,0), 
      "DPD_3_MS" NUMBER(38,0), 
      "TYPE" VARCHAR2(10 CHAR), 
      "PRIORITY" VARCHAR2(4 BYTE), 
      "CAMPAIGN_TYPE" VARCHAR2(10 BYTE), 
      "DATE_VALID_FROM" DATE, 
      "DATE_VALID_TO" DATE, 
      "SID_RESULT" VARCHAR2(15 BYTE), 
      "SCORE" NUMBER, 
      "RISK_GROUP" VARCHAR2(15 BYTE), 
      "RBP_SEGMENT" VARCHAR2(20 BYTE), 
      "LOST_ELIG_REASON" VARCHAR2(24 BYTE), 
      "DATE_CHECK" DATE, 
      "FLAG_STILL_ELIGIBLE" CHAR(1 BYTE)
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255  NOLOGGING 
  STORAGE(
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "AP_CRM" 
  PARTITION BY RANGE ("LOG_DATE") INTERVAL (NUMTOYMINTERVAL(1,'MONTH')) 
 (PARTITION "PART1"  VALUES LESS THAN (TO_DATE(' 2017-04-01 00:00:00', 'SYYYY-MM-DD HH24:MI:SS', 'NLS_CALENDAR=GREGORIAN')) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS NOLOGGING 
  STORAGE(INITIAL 8388608 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "AP_CRM" ) 

  PARALLEL  ENABLE ROW MOVEMENT ;
